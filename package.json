{
    "name": "ReactAmplify",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "android": "react-native run-android",
        "ios": "react-native run-ios",
        "start": "react-native start",
        "test": "jest",
        "lint": "eslint . --ext .jsx,.ts,.tsx",
        "lint:fix": "eslint . --ext .jsx,.ts,.tsx --fix",
        "prepare": "husky install",
        "amplify-modelgen": "node amplify/scripts/amplify-modelgen.js",
        "amplify-push": "node amplify/scripts/amplify-push.js"
    },
    "dependencies": {
        "@react-native-async-storage/async-storage": "1.17.7",
        "@react-native-community/netinfo": "9.3.0",
        "@react-native-picker/picker": "2.4.2",
        "@react-navigation/bottom-tabs": "^6.3.2",
        "@react-navigation/native": "6.0.11",
        "amazon-cognito-identity-js": "5.2.10",
        "aws-amplify": "4.3.27",
        "aws-amplify-react-native": "6.0.5",
        "graphql": "16.5.0",
        "lodash": "4.17.21",
        "react": "18.0.0",
        "react-native": "0.69.1",
        "react-native-biometrics": "file:../react-native-biometrics",
        "react-native-encrypted-storage": "4.0.2",
        "react-native-safe-area-context": "4.3.1",
        "react-native-screens": "3.15.0",
        "react-native-svg": "12.4.3"
    },
    "devDependencies": {
        "@babel/core": "7.12.9",
        "@babel/runtime": "7.12.5",
        "@react-native-community/eslint-config": "2.0.0",
        "@tsconfig/react-native": "2.0.2",
        "@types/jest": "26.0.23",
        "@types/lodash": "4.14.182",
        "@types/react-native": "0.69.3",
        "@types/react-test-renderer": "18.0.0",
        "@typescript-eslint/eslint-plugin": "5.29.0",
        "@typescript-eslint/parser": "5.30.7",
        "babel-jest": "26.6.3",
        "babel-plugin-module-resolver": "4.1.0",
        "eslint": "7.32.0",
        "eslint-plugin-filename-rules": "1.2.0",
        "husky": "8.0.1",
        "jest": "26.6.3",
        "lint-staged": "13.0.3",
        "metro-react-native-babel-preset": "0.70.3",
        "react-test-renderer": "18.0.0",
        "typescript": "4.4.4"
    },
    "jest": {
        "preset": "react-native",
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ]
    },
    "lint-staged": {
        "*.(ts|tsx)": [
            "yarn run lint:fix",
            "git add"
        ],
        "*.{ts,tsx}": "eslint --cache --fix"
    }
}
